{
    "root": true,
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
        "object-literal-sort-keys": 0,
        "interface-name": 0,
        "no-empty": 0,
        "no-console": 0,
        "radix": 0,
        "semi": 2,
        "no-unused-vars": 1,
        "no-unused-expressions": 1,
        "quotes": 2
    }
  }